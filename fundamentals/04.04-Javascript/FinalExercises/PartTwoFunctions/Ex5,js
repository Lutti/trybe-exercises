//Crie uma função que receba um array de inteiros e retorne o inteiro que mais se repete.

function queMaisSeRepete(arr)
{
    let arrSort = arr.Sort();
    let arrCount = [];

    for (let i in arr)
    {
        let count = 0;
        for (let i2 in arr)
        {
            if (arr[i] === arr[i2])
            {
                count += 1;
            }
        }
        arrCount.push(count);
    }
}